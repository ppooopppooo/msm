<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

  <!-- Title of the "Voicemail" settings screen, with a text label identifying which SIM the settings are for. -->
  <string name="voicemail_settings_with_label">"Голосовая почта (<xliff:g example="Mock Carrier" id="subscriptionlabel">%s</xliff:g>)</string>

  <!-- Call settings screen, setting option name -->
  <string name="voicemail_settings_title">Голосовая почта</string>

  <!-- Voicemail notifications title. The user clicks on this preference to select
         which sound to play and whether to vibrate when a voicemail notification is received.
         [CHAR LIMIT=30] -->
  <string name="voicemail_notifications_preference_title">Уведомления</string>

  <!-- Title for advanced settings in the voicemail settings -->
  <string name="voicemail_advanced_settings_title">Расширенные настройки</string>

  <!-- Visual voicemail on/off title [CHAR LIMIT=40] -->
  <string name="voicemail_visual_voicemail_switch_title">Визуальная голосовая почта</string>

  <!-- Visual voicemail archive on/off title [CHAR LIMIT=40] -->
  <string name="voicemail_visual_voicemail_auto_archive_switch_title">Доп. место хранения и резервирования</string>

  <!-- Voicemail change PIN dialog title [CHAR LIMIT=40] -->
  <string name="voicemail_set_pin_preference_title">Установка PIN-кода</string>
  <!-- Voicemail change PIN dialog title [CHAR LIMIT=40] -->
  <string name="voicemail_change_pin_preference_title">Изменение PIN-кода</string>

  <string name="voicemail_change_pin_preference_summary_disable">Чтобы изменить PIN-код, включите визуальную голосовую почту</string>
  <string name="voicemail_change_pin_preference_summary_not_activated">Визуальная голосовая почта пока не активирована, повторите попытку позже</string>

  <!-- Hint for the old PIN field in the change vociemail PIN dialog -->
  <string name="vm_change_pin_old_pin">Старый PIN-код</string>
  <!-- Hint for the new PIN field in the change vociemail PIN dialog -->
  <string name="vm_change_pin_new_pin">Новый PIN-код</string>

  <!-- Message on the dialog when PIN changing is in progress -->
  <string name="vm_change_pin_progress_message">Подождите…</string>
  <!-- Error message for the voicemail PIN change if the PIN is too short -->
  <string name="vm_change_pin_error_too_short">Новый PIN-код слишком короткий.</string>
  <!-- Error message for the voicemail PIN change if the PIN is too long -->
  <string name="vm_change_pin_error_too_long">Новый PIN-код слишком длинный.</string>
  <!-- Error message for the voicemail PIN change if the PIN is too weak -->
  <string name="vm_change_pin_error_too_weak">Новый PIN-код слишком простой. Надёжный пароль не должен содержать непрерывную последовательность чисел или их повторение.</string>
  <!-- Error message for the voicemail PIN change if the old PIN entered doesn't match  -->
  <string name="vm_change_pin_error_mismatch">Старый PIN-код указан неверно.</string>
  <!-- Error message for the voicemail PIN change if the new PIN contains invalid character -->
  <string name="vm_change_pin_error_invalid">Новый PIN-код содержит недопустимые символы.</string>
  <!-- Error message for the voicemail PIN change if operation has failed -->
  <string name="vm_change_pin_error_system_error">Не удалось сменить PIN-код</string>

  <!-- The title for the change voicemail PIN activity -->
  <string name="change_pin_title">Сменить PIN-код голосовой почты</string>
  <!-- The label for the continue button in change voicemail PIN activity -->
  <string name="change_pin_continue_label">Продолжить</string>
  <!-- The label for the cancel button in change voicemail PIN activity -->
  <string name="change_pin_cancel_label">Отмена</string>
  <!-- The title for the enter old pin step in change voicemail PIN activity -->
  <string name="change_pin_enter_old_pin_header">Подтвердите предыдущий PIN-код</string>
  <!-- The hint for the enter old pin step in change voicemail PIN activity -->
  <string name="change_pin_enter_old_pin_hint">Чтобы продолжить, введите PIN-код голосовой почты.</string>
  <!-- The title for the enter new pin step in change voicemail PIN activity -->
  <string name="change_pin_enter_new_pin_header">Введите новый PIN-код</string>
  <!-- The hint for the enter new pin step in change voicemail PIN activity -->
  <string name="change_pin_enter_new_pin_hint">PIN должен содержать <xliff:g example="4" id="min">%1$d</xliff:g>-<xliff:g example="7" id="max">%2$d</xliff:g> знаков.</string>
  <!-- The title for the confirm new pin step in change voicemail PIN activity -->
  <string name="change_pin_confirm_pin_header">Подтвердите PIN-код</string>
  <!-- The error message for th confirm new pin step in change voicemail PIN activity, if the pin doen't match the one previously entered -->
  <string name="change_pin_confirm_pins_dont_match">PIN-коды не совпадают.</string>
  <!-- The toast to show after the voicemail PIN has been successfully changed -->
  <string name="change_pin_succeeded">PIN-код голосовой почты обновлен</string>
  <!-- The error message to show if the server reported an error while attempting to change the voicemail PIN -->
  <string name="change_pin_system_error">Не удалось установить PIN-код</string>

  <!-- Title for visual voicemail setting that enables user to donate their voicemails for analysis.
       [CHAR LIMIT=40] -->
  <string name="voicemail_visual_voicemail_donation_switch_title">Анализ транскрипции голосовой почты</string>
  <!-- Summary information for visual voicemail donation setting when visual voicemail is not enabled
       [CHAR LIMIT=NONE] -->
  <string name="voicemail_donate_preference_summary_disable">Визуальная голосовая почта должна быть включена для передачи голосовой почты</string>
  <!-- Summary information for visual voicemail donation setting when visual voicemail is not activated
       [CHAR LIMIT=NONE] -->
  <string name="voicemail_donate_preference_summary_not_activated">Визуальная голосовая почта еще не активирована. Пожалуйста, попробуйте позже.</string>
  <!-- Summary information for visual voicemail donation setting [CHAR LIMIT=NONE] -->
  <string name="voicemail_donate_preference_summary_info">Позвольте Google просматривать сообщения голосовой почты чтобы улучшить точность транскрипции</string>

  <!-- Title for disable visual voicemail confirmation dialog [CHAR LIMIT=40] -->
  <string name="confirm_disable_voicemail_dialog_title">Отключить визуальную голосовую почту</string>
  <!-- Message explaining the implictions of disabling visual voicemail [CHAR LIMIT=NONE] -->
  <string name="confirm_disable_voicemail_dialog_message">Это удалит всю голосовую почту и транскрипции Google, хранящиеся в этом приложении. Ваш оператор может хранить свои собственные копии.</string>
  <!-- The label for the confirm-disable-voicemail button [CHAR LIMIT=16] -->
  <string name="confirm_disable_voicemail_accept_dialog_label">ВЫКЛЮЧИТЬ</string>
</resources>
